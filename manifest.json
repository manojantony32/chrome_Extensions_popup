{
  "manifest_version": 2,
  "name": "manoj extension with popup",
  "version": "0.01",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "chatIcon.png",
    "default_popup": "popup/popup.html",
    "default_title": "A Popup will come here"
  }
}
